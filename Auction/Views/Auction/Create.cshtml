@model Auction.Web.Models.AuctionArticleViewModel

@{
	ViewData["Title"] = "Create Auction";
}

<nav aria-label="breadcrumb">
	<ol class="breadcrumb">
		<li class="breadcrumb-item"><a asp-action="Index">Auction List</a></li>
		<li class="breadcrumb-item active" aria-current="page">New Auction</li>
	</ol>
</nav>

<div class="row">
	<div class="col-md-4">
		<form asp-action="Create" id="create-form" method="post">
			<partial name="_CreateEdit" />
